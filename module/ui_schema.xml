<?xml version='1.0' encoding='utf-8'?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>FILL THIS IN</h:title>
    <model>
      <instance>
        <faims id="FILL_THIS_IN">
          <!--START: Style-->
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <!--END: Style-->
          <User>
            <User>
              <User/>
              <Log_in/>
              <Sign_up/>
            </User>
          </User>
          <Control>
            <Main>
              <Take_Feature/>
              <Next_Object_ID/>
              <GPS_Diagnostics/>
            </Main>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_Types/>
              <Entity_List/>
            </Search>
            <Table>
              <Table/>
            </Table>
            <Map>
              <Map/>
              <Colgroup_1>
                <Col_1>
                  <Centre_Me/>
                </Col_1>
                <Col_2>
                  <Create_Shape/>
                </Col_2>
              </Colgroup_1>
            </Map>
          </Control>
          <Burial_Mound>
            <General>
              <Object_ID/>
              <Date/>
              <Burial_Mound_author/>
              <Type/>
              <Name_or_Other_ID/>
              <Source/>
              <Description/>
              <Surrounding_Landuse/>
              <Surface_Landuse/>
              <Prominence/>
              <Colgroup_1>
                <Col_1>
                  <Latitude/>
                </Col_1>
                <Col_2>
                  <Longitude/>
                </Col_2>
              </Colgroup_1>
              <Colgroup_2>
                <Col_1>
                  <Northing/>
                </Col_1>
                <Col_2>
                  <Easting/>
                </Col_2>
                <Col_3>
                  <Accuracy/>
                </Col_3>
              </Colgroup_2>
              <Take_From_GPS_1/>
              <Elevation/>
              <General_Notes/>
              <Go_to_Map/>
              <Facing_Direction/>
              <Mound_Overview_Photo/>
              <Mound_Overview_Photo_Button_1/>
            </General>
            <Dimensions>
              <Colgroup_1>
                <Col_1>
                  <Length_Max/>
                  <Width_Max/>
                  <Height_Max/>
                  <Area_Max/>
                </Col_1>
                <Col_2>
                  <Length_Min/>
                  <Width_Min/>
                  <Height_Min/>
                  <Area_Min/>
                </Col_2>
              </Colgroup_1>
              <Other_Dimension/>
              <Shape_Notes/>
              <Attach_a_Sketch/>
              <Attach_a_Sketch_Button_1/>
              <Sketch_Photo/>
              <Sketch_Photo_Button_1/>
            </Dimensions>
            <Material>
              <Surface_Material_Description/>
              <Surface_Material_Density/>
              <Sample_Collected/>
              <Stone_Feature_Present_And_Visible/>
              <Stone_Feature_Description/>
              <Material_Photo/>
              <Material_Photo_Button_1/>
            </Material>
            <CRM>
              <Condition/>
              <Disturbance_Description/>
              <Principal_Factors/>
              <Other_Sources_of_Impact/>
              <Age_of_Damage/>
              <RT_Number/>
              <RT_Kind/>
              <RT_Method/>
              <RT_Description/>
              <Volume_of_Soil_Removed_via_RT/>
              <Affect/>
              <Impact/>
              <Comments_and_Recommendations/>
              <CRM_Photo/>
              <CRM_Photo_Button_1/>
            </CRM>
          </Burial_Mound>
          <Shape>
            <Shape_Note>
              <Label/>
              <Note/>
            </Shape_Note>
          </Shape>
        </faims>
      </instance>
      <bind nodeset="/faims/Control/Main/Next_Object_ID" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/General/Elevation" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_1/Length_Max" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_1/Width_Max" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_1/Height_Max" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_1/Area_Max" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_2/Length_Min" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_2/Width_Min" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_2/Height_Min" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/Dimensions/Colgroup_1/Col_2/Area_Min" type="decimal"/>
      <bind nodeset="/faims/Burial_Mound/CRM/RT_Number" type="decimal"/>
    </model>
  </h:head>
  <h:body>
    <!--START: Style-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <!--END: Style-->
    <group ref="User">
      <label>{User}</label>
      <group ref="User" faims_scrollable="false">
        <label>{User}</label>
        <select1 ref="User">
          <label>{User}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <trigger ref="Log_in">
          <label>{Log_in}</label>
        </trigger>
        <trigger ref="Sign_up">
          <label>{Sign_up}</label>
        </trigger>
      </group>
    </group>
    <group ref="Control">
      <label>{Control}</label>
      <group ref="Main">
        <label>{Main}</label>
        <trigger ref="Take_Feature">
          <label>{Take_Feature}</label>
        </trigger>
        <input ref="Next_Object_ID" faims_style_class="required">
          <label>{Next_Object_ID}</label>
        </input>
        <input faims_read_only="true" ref="GPS_Diagnostics">
          <label>{GPS_Diagnostics}</label>
        </input>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Search}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="large">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search_Button}</label>
            </trigger>
          </group>
        </group>
        <select1 ref="Entity_Types">
          <label>{Entity_Types}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Table" faims_scrollable="false">
        <label>{Table}</label>
        <input ref="Table" faims_table="true">
          <label>{Table}</label>
        </input>
      </group>
      <group ref="Map" faims_scrollable="false">
        <label>{Map}</label>
        <input faims_map="true" ref="Map">
          <label>{Map}</label>
        </input>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <trigger ref="Centre_Me">
              <label>{Centre_Me}</label>
            </trigger>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <trigger ref="Create_Shape">
              <label>{Create_Shape}</label>
            </trigger>
          </group>
        </group>
      </group>
    </group>
    <group ref="Burial_Mound" faims_archent_type="Burial Mound">
      <label>{Burial_Mound}</label>
      <group ref="General">
        <label>{General}</label>
        <input faims_attribute_name="Object ID" faims_attribute_type="measure" ref="Object_ID" faims_style_class="required">
          <label>{Object_ID}</label>
        </input>
        <input faims_attribute_name="Date" faims_attribute_type="measure" ref="Date" faims_read_only="true">
          <label>{Date}</label>
        </input>
        <input faims_attribute_name="Burial Mound author" faims_attribute_type="measure" ref="Burial_Mound_author" faims_read_only="true">
          <label>{Author}</label>
        </input>
        <select1 faims_attribute_name="Type" faims_attribute_type="vocab" ref="Type">
          <label>{Type}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Name or Other ID" faims_attribute_type="measure" ref="Name_or_Other_ID">
          <label>{Name_or_Other_ID}</label>
        </input>
        <select1 faims_attribute_name="Source" faims_attribute_type="vocab" ref="Source">
          <label>{Source}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Description" faims_attribute_type="measure" ref="Description">
          <label>{Description}</label>
        </input>
        <select1 faims_attribute_name="Surrounding Landuse" faims_attribute_type="vocab" ref="Surrounding_Landuse">
          <label>{Surrounding_Landuse}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 faims_attribute_name="Surface Landuse" faims_attribute_type="vocab" ref="Surface_Landuse">
          <label>{Surface_Landuse}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 faims_attribute_name="Prominence" faims_attribute_type="vocab" ref="Prominence">
          <label>{Prominence}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Latitude" faims_attribute_type="measure" ref="Latitude" faims_read_only="true">
              <label>{Latitude}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Longitude" faims_attribute_type="measure" ref="Longitude" faims_read_only="true">
              <label>{Longitude}</label>
            </input>
          </group>
        </group>
        <group ref="Colgroup_2" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Northing" faims_attribute_type="measure" ref="Northing" faims_read_only="true">
              <label>{Northing}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Easting" faims_attribute_type="measure" ref="Easting" faims_read_only="true">
              <label>{Easting}</label>
            </input>
          </group>
          <group ref="Col_3" faims_style="even">
            <label/>
            <input faims_attribute_name="Accuracy" faims_attribute_type="measure" ref="Accuracy" faims_read_only="true">
              <label>{Accuracy}</label>
            </input>
          </group>
        </group>
        <trigger ref="Take_From_GPS_1">
          <label>{Take_From_GPS}</label>
        </trigger>
        <input faims_attribute_name="Elevation" faims_attribute_type="measure" ref="Elevation">
          <label>{Elevation__MASL_}</label>
        </input>
        <input faims_attribute_name="General Notes" faims_attribute_type="measure" ref="General_Notes">
          <label>{General_Notes}</label>
        </input>
        <trigger ref="Go_to_Map">
          <label>{Go_to_Map}</label>
        </trigger>
        <select1 faims_attribute_name="Facing Direction" faims_attribute_type="vocab" ref="Facing_Direction">
          <label>{Facing_Direction}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select faims_sync="true" type="camera" faims_attribute_name="Mound Overview Photo" faims_attribute_type="measure" ref="Mound_Overview_Photo">
          <label>{Mound_Overview_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="Mound_Overview_Photo_Button_1">
          <label>{Mound_Overview_Photo}</label>
        </trigger>
      </group>
      <group ref="Dimensions">
        <label>{Dimensions}</label>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Length Max" faims_attribute_type="measure" ref="Length_Max">
              <label>{Length_Max}</label>
            </input>
            <input faims_attribute_name="Width Max" faims_attribute_type="measure" ref="Width_Max">
              <label>{Width_Max}</label>
            </input>
            <input faims_attribute_name="Height Max" faims_attribute_type="measure" ref="Height_Max">
              <label>{Height_Max}</label>
            </input>
            <input faims_attribute_name="Area Max" faims_attribute_type="measure" ref="Area_Max">
              <label>{Area_Max}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Length Min" faims_attribute_type="measure" ref="Length_Min">
              <label>{Length_Min}</label>
            </input>
            <input faims_attribute_name="Width Min" faims_attribute_type="measure" ref="Width_Min">
              <label>{Width_Min}</label>
            </input>
            <input faims_attribute_name="Height Min" faims_attribute_type="measure" ref="Height_Min">
              <label>{Height_Min}</label>
            </input>
            <input faims_attribute_name="Area Min" faims_attribute_type="measure" ref="Area_Min">
              <label>{Area_Min}</label>
            </input>
          </group>
        </group>
        <input faims_attribute_name="Other Dimension" faims_attribute_type="measure" ref="Other_Dimension">
          <label>{Other_Dimension}</label>
        </input>
        <input faims_attribute_name="Shape Notes" faims_attribute_type="measure" ref="Shape_Notes">
          <label>{Shape_Notes}</label>
        </input>
        <select faims_sync="true" type="file" faims_attribute_name="Attach a Sketch" faims_attribute_type="measure" ref="Attach_a_Sketch">
          <label>{Attach_a_Sketch}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="Attach_a_Sketch_Button_1">
          <label>{Attach_a_Sketch}</label>
        </trigger>
        <select faims_sync="true" type="camera" faims_attribute_name="Sketch Photo" faims_attribute_type="measure" ref="Sketch_Photo">
          <label>{Sketch_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="Sketch_Photo_Button_1">
          <label>{Sketch_Photo}</label>
        </trigger>
      </group>
      <group ref="Material">
        <label>{Material}</label>
        <input faims_attribute_name="Surface Material Description" faims_attribute_type="measure" ref="Surface_Material_Description">
          <label>{Surface_Material_Description}</label>
        </input>
        <select1 faims_attribute_name="Surface Material Density" faims_attribute_type="vocab" ref="Surface_Material_Density">
          <label>{Surface_Material_Density}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Sample Collected" faims_attribute_type="vocab" ref="Sample_Collected">
          <label>{Sample_Collected_}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="full" faims_attribute_name="Stone Feature Present And Visible" faims_attribute_type="vocab" ref="Stone_Feature_Present_And_Visible">
          <label>{Stone_Feature_Present_And_Visible_}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Stone Feature Description" faims_attribute_type="measure" ref="Stone_Feature_Description">
          <label>{Stone_Feature_Description}</label>
        </input>
        <select faims_sync="true" type="camera" faims_attribute_name="Material Photo" faims_attribute_type="measure" ref="Material_Photo">
          <label>{Material_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="Material_Photo_Button_1">
          <label>{Material_Photo}</label>
        </trigger>
      </group>
      <group ref="CRM">
        <label>{CRM}</label>
        <select1 faims_attribute_name="Condition" faims_attribute_type="vocab" ref="Condition">
          <label>{Condition}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Disturbance Description" faims_attribute_type="measure" ref="Disturbance_Description">
          <label>{Disturbance_Description}</label>
        </input>
        <select1 faims_attribute_name="Principal Factors" faims_attribute_type="vocab" ref="Principal_Factors">
          <label>{Principal_Factors}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select faims_attribute_name="Other Sources of Impact" faims_attribute_type="vocab" ref="Other_Sources_of_Impact">
          <label>{Other_Sources_of_Impact}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select faims_attribute_name="Age of Damage" faims_attribute_type="vocab" ref="Age_of_Damage">
          <label>{Age_of_Damage}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <input faims_attribute_name="RT Number" faims_attribute_type="measure" ref="RT_Number">
          <label>{RT_Number}</label>
        </input>
        <select faims_attribute_name="RT Kind" faims_attribute_type="vocab" ref="RT_Kind">
          <label>{RT_Kind}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <select faims_attribute_name="RT Method" faims_attribute_type="vocab" ref="RT_Method">
          <label>{RT_Method}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <input faims_attribute_name="RT Description" faims_attribute_type="measure" ref="RT_Description">
          <label>{RT_Description}</label>
        </input>
        <select1 faims_attribute_name="Volume of Soil Removed via RT" faims_attribute_type="vocab" ref="Volume_of_Soil_Removed_via_RT">
          <label>{Volume_of_Soil_Removed_via_RT}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 faims_attribute_name="Affect" faims_attribute_type="vocab" ref="Affect" faims_style_class="required">
          <label>{Affect}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 faims_attribute_name="Impact" faims_attribute_type="vocab" ref="Impact" faims_style_class="required">
          <label>{Impact}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Comments and Recommendations" faims_attribute_type="measure" ref="Comments_and_Recommendations">
          <label>{Comments_and_Recommendations}</label>
        </input>
        <select faims_sync="true" type="camera" faims_attribute_name="CRM Photo" faims_attribute_type="measure" ref="CRM_Photo">
          <label>{CRM_Photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="CRM_Photo_Button_1">
          <label>{CRM_Photo}</label>
        </trigger>
      </group>
    </group>
    <group ref="Shape" faims_archent_type="Shape">
      <label>{Shape}</label>
      <group ref="Shape_Note">
        <label>{Shape_Note}</label>
        <input faims_attribute_name="Label" faims_attribute_type="measure" ref="Label">
          <label>{Label}</label>
        </input>
        <input faims_attribute_name="Note" faims_attribute_type="measure" ref="Note">
          <label>{Note}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
